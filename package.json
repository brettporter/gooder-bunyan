{
  "name": "gooder-bunyan",
  "version": "3.0.0",
  "description": "hapi good-reporter to bunyan logging adapter",
  "main": "lib/index.js",
  "scripts": {
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lancespeelmon/gooder-bunyan.git"
  },
  "keywords": [
    "bunyan",
    "hapi",
    "good",
    "reporter",
    "logger",
    "adapter"
  ],
  "author": "Lance Speelmon <lance@speelmon.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lancespeelmon/gooder-bunyan/issues"
  },
  "homepage": "https://github.com/lancespeelmon/gooder-bunyan",
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "del": "^2.2.0",
    "gulp": "^3.8.11",
    "gulp-coffee": "^2.3.1",
    "gulp-coffee-istanbul": "^0.9.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-mocha": "^2.0.0",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-util": "^3.0.4",
    "traceur": "0.0.111"
  },
  "dependencies": {},
  "peerDependencies": {
    "good": "7.x.x"
  },
  "coffeelintConfig": {
    "arrow_spacing": {
      "name": "arrow_spacing",
      "level": "error"
    },
    "max_line_length": {
      "name": "max_line_length",
      "value": 150,
      "level": "warn",
      "limitComments": false
    },
    "indentation": {
      "name": "indentation",
      "value": 2,
      "level": "warn"
    },
    "no_empty_param_list": {
      "name": "no_empty_param_list",
      "level": "ignore"
    },
    "cyclomatic_complexity": {
      "name": "cyclomatic_complexity",
      "value": 22,
      "level": "error"
    },
    "no_unnecessary_fat_arrows": {
      "name": "no_unnecessary_fat_arrows",
      "level": "error"
    }
  }
}
